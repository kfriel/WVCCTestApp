<Window x:Name="WVCCMainWindow" x:Class="WVCCTestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WVCC Call Management" 
        Height="750" 
        Width="958" 
        WindowStyle="ToolWindow" 
        WindowStartupLocation="CenterScreen" Uid="MainWindow">
    <Grid x:Name="Grid1" 
          Margin="5,5,-8,-49" 
          ScrollViewer.VerticalScrollBarVisibility="Disabled" 
          MinWidth="798" 
          MinHeight="663" 
          MaxWidth="1440" 
          MaxHeight="900" 
          RenderTransformOrigin="0.382,0.166">
        <Grid.RowDefinitions>
            <RowDefinition Height="101*"/>
            <RowDefinition Height="670*"/>
        </Grid.RowDefinitions>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" 
                                 MappingMode="RelativeToBoundingBox" 
                                 StartPoint="0.5,0">
                <GradientStop Color="Black" 
                              Offset="1"/>
                <GradientStop Color="#FF5194F3"/>
            </LinearGradientBrush>
        </Grid.Background>
        <!-- Mode Control -->
        <Border BorderBrush="Black" 
                BorderThickness="1" 
                HorizontalAlignment="Left" 
                Height="58" 
                Margin="22,6,0,0" 
                VerticalAlignment="Top" 
                Width="191"/>
        <Label Content="Mode" 
               HorizontalAlignment="Left" 
               Margin="147,8,0,0" 
               VerticalAlignment="Top" Height="23" Width="36"/>
        <ComboBox x:Name="Mode" 
                  HorizontalAlignment="Left" 
                  Margin="27,10,0,0" 
                  VerticalAlignment="Top" 
                  Width="120"
                  Loaded="Mode_menu_pl"
                  SelectionChanged="Mode_SelectionChanged" 
                  IsEditable="False" 
                  ToolTip="Mode: Defaulted to &quot;Off Line&quot;. Please select the the tool mode you whish to work in." Height="21">
            <ComboBox.Resources>
                <Style TargetType="{x:Type ComboBox}">
                    <Setter Property="StaysOpenOnEdit" 
                            Value="True"/>
                    <Setter Property="IsDropDownOpen" 
                            Value="False"/>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" 
                                 Value="True">
                            <Setter Property="IsDropDownOpen" 
                                    Value="True"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </ComboBox.Resources>
        </ComboBox>
        <!-- Assignment Control-->
        <Label Content="Assignment" 
               HorizontalAlignment="Left" 
               Margin="147,33,0,0" 
               VerticalAlignment="Top" Height="23" Width="66"/>
        <ComboBox x:Name="Assignmentcbx" 
                  HorizontalAlignment="Left" 
                  Margin="27,30,0,0" 
                  VerticalAlignment="Top" 
                  Width="120" 
                  Loaded="Assignment_menu_pl"
                  SelectionChanged="Assignment_SelectionChanged" 
                  BorderThickness="0,5,0,0" 
                  IsReadOnly="True" Height="26">
            <ComboBox.Resources>
                <Style TargetType="{x:Type ComboBox}">
                    <Setter Property="StaysOpenOnEdit" 
                            Value="True"/>
                    <Setter Property="IsDropDownOpen" 
                            Value="False"/>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" 
                                 Value="True">
                            <Setter Property="IsDropDownOpen" 
                                    Value="True"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </ComboBox.Resources>
        </ComboBox>
