                   Content="{Binding Path=NotifyMessage, UpdateSourceTrigger=PropertyChanged}" 
                   Visibility="Hidden"
                   HorizontalAlignment="Center" 
                   Height="39" 
                   VerticalAlignment="Center" 
                   Width="239" 
                   FontWeight="Bold" 
                   FontSize="18" 
                   VerticalContentAlignment="Center" 
                   HorizontalContentAlignment="Center" 
                   Foreground="#FF26EA07"/>
        </Border>
        <!-- Call List Management-->
        <ListView 
            Name="ListView"
            ItemsSource = "{Binding}"
            SelectionChanged="CallListSlChnd"
            SelectedIndex="-1" 
            SelectionMode="Single"
            HorizontalAlignment="Left" 
            Height="237" 
            Margin="22,66,0,0" 
            VerticalAlignment="Top" 
            Width="191" 
            Cursor="Hand" 
            BorderThickness="3" 
            SnapsToDevicePixels="True" 
            ToolTip="Select Veteran to call." 
            Grid.RowSpan="2">
            <ListView.View>
                <GridView>
                    <GridViewColumn 
                        x:Name="LVWVCCID" 
                        Header="ID" 
                        Width="0"
                        DisplayMemberBinding="{Binding WVCCID}"/>
                    <GridViewColumn 
                        x:Name="First" 
                        Header="First" 
                        Width="57"
                        DisplayMemberBinding="{Binding FName}"/>
                    <GridViewColumn 
                        x:Name="Last" 
                        Header="Last" 
                        Width="67"
                        DisplayMemberBinding="{Binding LName}"/>
                    <GridViewColumn 
                        x:Name="TZ" 
                        Header="TZ" 
                        Width="32"
                         DisplayMemberBinding="{Binding TZ}"/>
                </GridView>
            </ListView.View>
        </ListView>
